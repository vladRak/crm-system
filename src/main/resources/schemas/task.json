{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "/schemas/task.json",
  "required": ["taskname","taskdate"],
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "taskname": {
      "type": "string",
      "maxLength": 255
    },
    "taskdate": {
      "type": "integer",
      "maxLength": 13
    }
  }
}

